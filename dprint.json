{
  "lineWidth": 120,
  "useTabs": false,
  "indentWidth": 2,
  "typescript": {
  },
  "json": {
  },
  "markdown": {
  },
  "toml": {
  },
  "includes": [
    "**/*.{ts,tsx,js,jsx,cjs,mjs}",
    "**/*.{json,md,toml}"
  ],
  "excludes": [
    "**/.direnv",
    "**/.next",
    "**/node_modules",
    "**/*-lock.json",
    "**/next-env.d.ts"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.74.0.wasm",
    "https://plugins.dprint.dev/json-0.15.3.wasm",
    "https://plugins.dprint.dev/markdown-0.13.3.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm"
  ]
}
